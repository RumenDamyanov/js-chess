# Environment Configuration for js-chess

# Backend API Configuration
CHESS_API_URL=http://localhost:8080
CHESS_WS_URL=ws://localhost:8080

# Development Ports
LANDING_PORT=3000
VANILLA_PORT=3001
JQUERY_PORT=3002
VUE_PORT=3003
REACT_PORT=3004
ANGULAR_PORT=3005

# Backend Configuration
CHESS_HOST=0.0.0.0
CHESS_PORT=8080
CHESS_LOG_LEVEL=info

# LLM AI Configuration (for go-chess backend)
# Uncomment and configure the providers you want to use

# OpenAI Configuration
# OPENAI_API_KEY=your_openai_api_key_here
# OPENAI_MODEL=gpt-4

# Anthropic Configuration
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# ANTHROPIC_MODEL=claude-3-opus-20240229

# Google Gemini Configuration
# GOOGLE_API_KEY=your_google_api_key_here
# GOOGLE_MODEL=gemini-pro

# xAI Grok Configuration
# XAI_API_KEY=your_xai_api_key_here
# XAI_MODEL=grok-beta

# DeepSeek Configuration
# DEEPSEEK_API_KEY=your_deepseek_api_key_here
# DEEPSEEK_MODEL=deepseek-chat

# Production Configuration
NODE_ENV=development
CHESS_ENV=development

# Docker Configuration
DOCKER_BUILDKIT=1
COMPOSE_DOCKER_CLI_BUILD=1
